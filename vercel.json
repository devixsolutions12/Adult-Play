{
  "version": 2,
  "headers": [
    {
      "source": "/apk/(.*)",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/vnd.android.package-archive"
        },
        {
          "key": "Content-Disposition",
          "value": "attachment; filename=\"Adult Play.apk\""
        }
      ]
    },
    {
      "source": "/(.*\\.(js|css|jpg|jpeg|png|gif|svg|ico|woff|woff2|ttf|eot))",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}

